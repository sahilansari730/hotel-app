<link href="style/placeform.css" rel="stylesheet" type="text/css"/>
<div class="jumbotron">
    <h2>Choose Your Place</h2>    
    <div class="row">
        <div class="col-sm-6">
            <div class="form-group">
                    <label>Name:</label>
                    <input type="text" class="form-control" id="name" placeholder="Enter Your Name">
             </div> 
            <div class="form-group">
                <label>Choose Date To Check Availability:</label>
                <input type="date"  class="form-control">
           </div> 
             <div class="form-group">
                <label>Choose Adult:</label>
                <select class="form-control" id="sel1">
                 <option>Choose</option> 
                  <option>1</option>
                  <option>2</option>
                  <option>3</option>
                  <option>4</option>
                </select>
           </div> 
            <div class="form-group">
                <label>Choose Max Price</label>
                <input type="Text"  class="form-control" placeholder="Max Price (Rs)">
           </div> 
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                <label>Email:</label>
                <input type="text" class="form-control" placeholder="Enter Your Email">
            </div><br>
            <div class="form-group">
                 <input type="date"  class="form-control">
           </div> 
             <div class="form-group">
                <label>Choose Child:</label>
                <select class="form-control" id="sel2">
                  <option>Choose</option>
                  <option>1</option>
                  <option>2</option>
                  <option>3</option>
                  <option>4</option>
                </select>
           </div> 
            <br>
            <button type="button" class="btn btn-danger"><b>Check Availability</b></button>
        </div>
    </div>
    
    
    
</div>
  